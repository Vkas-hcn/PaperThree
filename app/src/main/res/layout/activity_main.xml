<?xml version="1.0" encoding="utf-8"?>
<layout>


    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#202227"
        tools:context=".b.ac.MainActivity">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context=".MainActivity">


            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/wallPaperThreeMainTitle"
                android:layout_width="150dp"
                android:layout_height="22dp"
                android:layout_marginStart="28dp"
                android:layout_marginTop="49dp"
                android:src="@mipmap/wallpaper"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/wallPaperThreeMainSetRl"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_centerInParent="true"
                android:layout_marginEnd="28dp"
                android:src="@mipmap/pager_three_set"
                app:layout_constraintBottom_toBottomOf="@id/wallPaperThreeMainTitle"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/wallPaperThreeMainTitle" />


            <com.youth.banner.Banner
                android:id="@+id/wallPaperThreeMainBanner"
                android:layout_width="match_parent"
                android:layout_height="156dp"
                android:layout_marginHorizontal="24dp"
                android:layout_marginTop="28dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/wallPaperThreeMainTitle" />

            <FrameLayout
                android:background="@mipmap/qqqaqaqa"
                android:layout_width="match_parent"
                android:layout_height="211dp"
                android:id="@+id/native_frame_qr"
                android:layout_margin="20dp"
                app:layout_constraintTop_toBottomOf="@id/wallPaperThreeMainBanner"
                />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/wallPaperThreeMainImgList"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginHorizontal="10dp"
                android:layout_marginTop="20dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="@id/native_frame_qr" />



        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>
