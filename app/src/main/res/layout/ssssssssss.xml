<?xml version="1.0" encoding="utf-8"?>
<layout>


    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"

        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#202227">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context=".b.ac.MainActivity">

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/pageTwoBg"
                android:layout_width="match_parent"
                android:layout_height="646dp"
                android:scaleType="centerCrop"
                android:src="@drawable/bg_ssssssss"
                app:layout_constraintTop_toTopOf="parent" />



            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/name"
                android:layout_width="wrap_content"
                android:layout_height="22dp"
                android:layout_marginBottom="23dp"
                android:src="@drawable/wallpaper"
                app:layout_constraintBottom_toTopOf="@id/clakemcpb"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <ProgressBar
                android:id="@+id/clakemcpb"
                style="@style/Widget.AppCompat.ProgressBar.Horizontal"
                android:layout_width="match_parent"
                android:layout_height="6dp"
                android:layout_marginHorizontal="73dp"
                android:layout_marginBottom="69dp"
                android:progressDrawable="@drawable/kkk"
                app:layout_constraintBottom_toBottomOf="parent"
                tools:progress="80" />


        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
